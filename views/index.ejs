<%- include('./partials/header') %> 


<h1>home</h1>

<section class="main">
    <% allmoviedata.forEach(ele => { %>
    <article>
        <a href="/movieDetails/<%= ele.id %>">
            <img src="https://image.tmdb.org/t/p/w500<%= ele.poster_path %> " alt="">
            <h3><%= ele.title%> </h3>
        </a>
        <p><%= ele.overview %> </p>
    </article>
    <% }) %>
</section>

<%- include('./partials/footer') %> 




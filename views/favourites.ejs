<%- include('./partials/header') %> 






<h1>MySHOWS</h1>
<section class="main">
    <% Movies.forEach(element => { %>
    <article>
      
            <img src="https://image.tmdb.org/t/p/w500<%= element.poster_path %> " alt="">
